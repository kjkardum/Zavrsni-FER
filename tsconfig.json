{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "component.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "lib": ["es2018", "dom"],
    "noImplicitAny": false
  },
  "externals": {
    "$openapiSpec": "openapiSpec"
  },
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
